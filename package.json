{
  "name": "react-webpack",
  "version": "1.0.0",
  "description": "simple react webpack application",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && webpack-dev-server --content-base src/ --inline",
    "build:prod": "webpack -d && cp src/index.html dist/index.html"
  },
  "keywords": [
    "reatjs",
    "redux"
  ],
  "author": "Basavakumar",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "jquery": "^3.2.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  }
}
